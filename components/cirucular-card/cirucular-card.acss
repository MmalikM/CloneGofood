.type-img{
  display: flex;
  flex-direction: column;
  flex-shrink:0;
  flex-grow:0;
  width: 70px;
  height: 70px;
  margin: 20rpx;
  border-radius: 100%;
}

.type-img image{
  width: 70px;
  height: 70px;
  border-radius: 100%;
}

.text-list{
  display: flex;
  justify-content: center;
  font-size: 25rpx;
  margin-bottom: 20rpx;
}

